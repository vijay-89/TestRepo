<messaging:emailTemplate subject=" {!relatedTo.Email_Subject__c}- {!relatedTo.Account__c}" recipientType="User" relatedToType="SBC_SOB_Request__c">
<messaging:HTMLEmailBody >
A SBC Request has been created in Salesforce.
<br/><br/>
 <table>
  <col width="300px">
  <col width="250px">
  <style>
    table, th, td {
        border: 1px solid black;
        border-collapse: collapse;
    }
    tr {
        padding-left: 30px;
        padding-right: 30px;
    }
    table.bdrless ,table.bdrless td{
        width:600px;
        border: none !important;
    }
</style>
  <tr>
     <td style="background-color:#89b5f0"><b>Custom SBC/SOB Request #<br/></b></td>
     <td>
     <apex:outputlink value="{!LEFT($Api.Partner_Server_URL_140,FIND('.com',$Api.Partner_Server_URL_140)+4)+relatedTo.Id}">{!relatedTo.Name}</apex:outputlink>
     </td> 
  </tr>
  <tr >
     <td style="background-color:#89b5f0"><b>Implementation Record ID<br/></b></td>
     <td>
        <apex:outputField value="{!relatedTo.Opportunity_ICR_ID_s__c}">
        </apex:outputField>
     </td>
  </tr>
  <tr >
     <td style="background-color:#89b5f0"><b>Account<br/></b></td>
