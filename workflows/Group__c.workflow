    <rules>
        <fullName>ANA_WF_GRP01_Email Group Services to get new Group Number for CS90</fullName>
        <actions>
            <name>GRP01_NewGroupNumberEmailAlertCS90</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Group__c.GroupNumberRequestStatus__c</field>
            <operation>equals</operation>
            <value>Requested</value>
        </criteriaItems>
        <criteriaItems>
            <field>Group__c.ClaimSystem__c</field>
            <operation>equals</operation>
            <value>CS90</value>
        </criteriaItems>
        <criteriaItems>
            <field>User.BypassWorkflow__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <description>BW 2015 
This email to Group Services indicates that a new Group is ready for a new Group Number</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
