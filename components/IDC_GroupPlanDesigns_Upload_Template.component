          <span class="slds-checkbox--faux"></span>
        </label>
		</th>
        
    	<td class="wrapword" style= "white-space:Normal">{{p.GroupSuffix}}</td>
		<td class="wrapword" style= "white-space:Normal">{{p.PlanName}}</td>
		<td class="wrapword" style= "white-space:Normal">{{p.LineOFBusiness}}</td>
		<td class="wrapword" style= "white-space:Normal">{{p.EmpCategory}}</td>
		<td class="wrapword" style= "white-space:Normal">{{p.EmpClassname}}</td>
		<td class="wrapword" style= "white-space:Normal">{{p.EffectiveDate}}</td>
		</tr>
     </tbody>
	 </table>
	</div>
</script>
    
