<script type="text/ng-template" id="vlcActStrcQuesEmpClassQliGpd.html">
<div class="panel panel-default" ng-controller="EmpClassQliGpdController as ctrl"  style="width: 80%">

  <!-- Start | code for setting all effective date | Date : 23rd Nov 2019 | Changed by Accenture-Panthers -->
    Set All Effective Date :
    <select id="setEffectiveDate" name="setEffectiveDate" ng-model="setEffectiveDateOption">
        <option value="Individual" selected>Individual</option>
        <option value="All">All</option>
    </select>
    <!--<input style="font-size:12px;" ng-show="setEffectiveDateOption=='All'" type="date" ng-change="ctrl.setAllEffectiveDate();" ng-blur="ctrl.changeAllEffectiveDate();" onkeydown="return false" ng-model="allDate"/>-->
    <input style="font-size:12px;" ng-show="setEffectiveDateOption=='All'" type="date" ng-change="ctrl.changeAllEffectiveDate();" ng-blur="ctrl.setAllEffectiveDate();" onkeydown="return false" ng-model="allDate"/>
    <br/>    
    <br/> 
    <!-- End | code for setting all effective date | Date : 23rd Nov 2019 | Changed by Accenture-Panthers -->
  <table class="table slds-table slds-table--bordered slds-table--cell-buffer" style="table-layout: auto;" width="100%">
  <thead>
    <tr>
     <!-- Header for checkbox -->
     <th  style="font-size:12px;">Select<br/></th>     
     <th class="slds-cell-shrink" style="font-size:12px;" ng-click="ctrl.sortBy('PlanName')">Plan Name
     <span class="sortorder" ng-show="propertyName === 'PlanName'" ng-class="{reverse: reverse}"></span>
     </th>     
     <th class="slds-cell-shrink" style="font-size:12px;"  ng-click="ctrl.sortBy('ProductLOB')">Line Of<br/>Business
     <span class="sortorder" ng-show="propertyName === 'ProductLOB'" ng-class="{reverse: reverse}"></span>
     </th>  
     <th class="slds-cell-shrink" style="font-size:12px;" ng-click="ctrl.sortBy('Funding')">Funding
     <span class="sortorder" ng-show="propertyName === 'Funding'" ng-class="{reverse: reverse}"></span>
     </th>   
     <th class="slds-cell-shrink" style="font-size:12px;" ng-click="ctrl.sortBy('ContractCode')">Contract<br/>Code
     <span class="sortorder" ng-show="propertyName === 'ContractCode'" ng-class="{reverse: reverse}"></span>
     </th>    
     <th class="slds-cell-shrink" style="font-size:12px;" ng-click="ctrl.sortBy('EmpCategory')">Employee<br/>Category
     <span class="sortorder" ng-show="propertyName === 'EmpCategory'" ng-class="{reverse: reverse}"></span>
     </th>       
     <th class="slds-cell-shrink" style="font-size:12px;" ng-click="ctrl.sortBy('EmpClassName')">Employee<br/>Class Name
     <span class="sortorder" ng-show="propertyName === 'EmpClassName'" ng-class="{reverse: reverse}"></span>
     </th>    
     <!--<th class="slds-cell-shrink" style="font-size:12px;" ng-click="ctrl.sortBy('GPDName')">GPD ID
     <span class="sortorder" ng-show="propertyName === 'GPDName'" ng-class="{reverse: reverse}"></span> -->
     </th> 
     <th class="slds-cell-shrink" style="font-size:12px;">Sequence<br/>Number</th> 
     <th class="slds-cell-shrink" style="font-size:12px;">Group Suffix<br/>Effective Date</th> 
     <th class="slds-cell-shrink" style="font-size:12px;">Group Suffix<br/>Cancel Date</th>  
     <th class="slds-cell-shrink" style="font-size:12px;" ng-click="ctrl.sortBy('GroupPlanDescription')">Group Plan<br/>Description
     <span class="sortorder" ng-show="propertyName === 'GroupPlanDescription'" ng-class="{reverse: reverse}"></span>
     </th>     
    </tr>
   </thead>

  
      
  <tbody class='ins' ng-if="control.vlcSI[control.itemsKey].length > 0" >
    <tr ng-repeat="gpdItem in (control.vlcSI[control.itemsKey] | orderBy:propertyName:reverse)" ng-class="{'vlc-selectableitem-selected' : gpdItem.vlcSelected }" >
      
       <th class="slds-cell-shrink" style="font-size:12px;">
        <label class="slds-checkbox">
           <input type="checkbox" ng-model='gpdItem.vlcSelected' 
                       ng-change='ctrl.selectUnselectCheckBox($index,gpdItem)'
                       ng-init='onSelectItem(control, gpdItem, $index, this, false)' />
         <span class="slds-checkbox--faux"></span>

        </label>
     </th>
          <td style="width:500px;overflow-x: hidden; overflow-y: scroll,auto;text-overflow:ellipsis;height:64px;white-space: initial;font-size:12px;">{{gpdItem.PlanName}}</td> 
          <td style="font-size:12px;">{{gpdItem.ProductLOB}}</td>                                                  
          <td style="font-size:12px;">{{gpdItem.Funding}}</td>
          <td style="font-size:12px;">{{gpdItem.ContractCode}}</td>
          <td style="width:500px;overflow-x: hidden; overflow-y: scroll,auto;text-overflow:ellipsis;height:64px;white-space: initial;font-size:12px;">{{gpdItem.EmpCategory}}</td>          
           <td style="width:500px;overflow-x: hidden; overflow-y: scroll,auto;text-overflow:ellipsis;height:64px;white-space: initial;font-size:12px;">{{gpdItem.EmpClassName}}</td>    
          <!-- <td style="font-size:12px;">{{gpdItem.GPDName}}</td> -->
          <td style="padding: 12px;font-size:12px;">    
            <p ng-show="!gpdItem.vlcSelected || !ctrl.checkEditAcces(gpdItem)" style="font-size:12px;">{{ gpdItem.SequenceNumber }}</p>
           <input type="text" name="SequenceNumber" ng-show="gpdItem.vlcSelected && ctrl.checkEditAcces(gpdItem)" ng-model="gpdItem.SequenceNumber"
           style="resize:none;font-size:12px;width:50px;"/>
           
          </td>
         
        <td style="font-size:12px;" ng-init="ctrl.convertEffectiveDateStrToDate()">
        
         <p ng-show="!gpdItem.vlcSelected">{{ gpdItem.GroupSuffixEffectiveDate | date :  "MM/dd/yyyy" }}</p>
         
           <input type="date" ng-model="gpdItem.GroupSuffixEffectiveDate"  ng-change="ctrl.changeEffDate($index,gpdItem)"
                        ng-show="gpdItem.vlcSelected" ng-required="gpdItem.vlcSelected" onkeydown="return false"
                        style="width: 112px;"
                        ng-style="{'border' : (gpdItem.vlcSelected == true) ? '1px solid #ff0000' : 'none'}"/>
         
            
         </td>
         
          <td style="font-size:12px;" ng-init="ctrl.convertCancelDateStrToDate()">
                 
         <p ng-show="!gpdItem.vlcSelected">{{ gpdItem.GroupSuffixCancelDate | date :  "MM/dd/yyyy" }}</p>
         
           <input type="date" ng-model="gpdItem.GroupSuffixCancelDate"  ng-change="ctrl.changeCancelDate($index,gpdItem)" onkeydown="return false"
                        ng-show="gpdItem.vlcSelected" style="width: 117px;"/>
     
         </td>
           
          <td style="text-align:center;">          
           <textarea name="text" wrap="soft" ng-readonly="!gpdItem.vlcSelected" ng-model="gpdItem.GroupPlanDescription"
           style="resize:none;font-size:12px"/>
           
          </td>
     </tr>
       
     </tbody>
     </table>
</div>


 <p>&nbsp;</p>
</script>

