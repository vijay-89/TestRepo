<script type="text/ng-template" id="vlcActStrcQuesQliProductData.html">
 <div class="panel panel-default" ng-controller="EmpClassQliGpdController as ctrl"  style="width: 100%">
 
  <table class="table slds-table slds-table--bordered slds-table--cell-buffer" style="table-layout: auto;" width="100%">
  <thead>
    <tr>   
    
     <th class="slds-cell-shrink" style="font-size:12px;">Plan Name</th>    
     <th class="slds-cell-shrink" style="font-size:12px;">Line Of<br/>Business</th>    
     <th class="slds-cell-shrink" style="font-size:12px;">Members</th>    
     <th class="slds-cell-shrink" style="font-size:12px;">Contract<br/>Code</th>    
     <th class="slds-cell-shrink" style="font-size:12px;">Product<br/>Stage</th>
     <th class="slds-cell-shrink" style="font-size:12px;">Alphanumeric<br/>Prefix Network</th> 
     <th class="slds-cell-shrink" style="font-size:12px;">Claims Route<br/>Code</th>
     <!-- PRDCRM2-1440-->
     <th class="slds-cell-shrink" style="font-size:12px;">Claims Grace<br/>Days</th>
     <!-- EOF PRDCRM2-1440-->
    </tr>
   </thead>
  
      
  <tbody class='ins' ng-if="control.vlcSI[control.itemsKey].length > 0" >
    <tr ng-repeat="p in control.vlcSI[control.itemsKey] track by $index" ng-init="ctrl.checkRouteCodeValidation(control)">
          <td style="width:500px;overflow-x: hidden; overflow-y: scroll,auto;text-overflow:ellipsis;height:64px;white-space: initial;font-size:12px;">{{p.ProductName}}</td> 
          <td style="font-size:12px;">{{p.ProductLOB}}</td>
          <td style="font-size:12px;">{{p.Members}}</td>   
          <td style="font-size:12px;">{{p.ContractCodeASF}}</td>
          <td style="font-size:12px;">{{p.ProductStage}}</td>
          <td>
            <div class="select-custom-element">
            
              <select ng-if="bpTree.response['APNs'].length==undefined" 
                name="apnName" id="apnName" class="slds-select ng-pristine ng-empty ng-valid ng-valid-required ng-touched" 
                style="font-size:12px;max-width:100%" 
                ng-model="p.APN"
                ng-change="ctrl.addQLItoJSON(p); ">
                    <option value="">-- Please Select --</option>
                    <option value="{{bpTree.response.APNs.APNId}}">{{bpTree.response.APNs.AlphanumericPrefixProductsName}}</option>
            </select>
            
            
            <select ng-if="bpTree.response['APNs'].length > 1"  name="apnName" id="apnName" class="slds-select ng-pristine ng-empty ng-valid ng-valid-required ng-touched" 
                 style="font-size:12px;max-width:100%;" 
                 ng-model="p.APN" ng-options="item.APNId as item.AlphanumericPrefixProductsName for item in bpTree.response['APNs']"
                 ng-change="ctrl.addQLItoJSON(p)">
                    <option value="">-- Please Select --</option>
                 </select>
                
            </div>
          </td>
          <td style="font-size:12px;"> 
			<span class="vlc-asterix icon-v-asterix vlc-editblock-asterix ng-scope" ng-show="!p.ClaimsRouteCode"></span>
            <input type="text" style="height:30px;width:74%;" ng-required="true" ng-model="p.ClaimsRouteCode" ng-change="ctrl.addQLItoJSON(p);ctrl.checkRouteCodeValidation(control)"/>  
          </td>
            <!-- PRDCRM2-1440-->
           <td>
                
            <!-- 30 days -->
            <div class="select-custom-element" ng-if="p.ClaimsGraceDays == '' && (p.ClaimsGraceDayState == 'California' || p.ClaimsGraceDayState == 'Colorado'|| p.ClaimsGraceDayState == 'Connecticut'|| p.ClaimsGraceDayState == 'Kentucky'|| p.ClaimsGraceDayState == 'Maine'|| p.ClaimsGraceDayState == 'Missouri'|| p.ClaimsGraceDayState == 'New Hampshire'|| p.ClaimsGraceDayState == 'Nevada'|| p.ClaimsGraceDayState == 'Ohio')" >
                <select 
                    name="ClaimsGraceDays" id="ClaimsGraceDays" class="slds-select ng-pristine ng-empty ng-valid ng-valid-required ng-touched" 
                    style="font-size:12px;max-width:100%" 
                    ng-model="p.ClaimsGraceDays"
                    ng-options="option.value as option.name for option in ClaimsGraceDays"
                    ng-init="p.ClaimsGraceDays = ClaimsGraceDays[0].value; onSelectItem(control, p, $index, this,true)"
                    ng-change="ctrl.addQLItoJSON(p)">
                </select>
            </div>
              
            <!-- 31 days -->
            <div class="select-custom-element" ng-if="p.ClaimsGraceDays == '' && (p.ClaimsGraceDayState == 'Georgia' || p.ClaimsGraceDayState == 'Indiana'|| p.ClaimsGraceDayState == 'Virginia'|| p.ClaimsGraceDayState == 'Wisconsin')" >
                <select 
                    name="ClaimsGraceDays" id="ClaimsGraceDays" class="slds-select ng-pristine ng-empty ng-valid ng-valid-required ng-touched" 
                    style="font-size:12px;max-width:100%" 
                    ng-model="p.ClaimsGraceDays"
                    ng-options="option.value as option.name for option in ClaimsGraceDays"
                    ng-init="p.ClaimsGraceDays = ClaimsGraceDays[1].value; onSelectItem(control, p, $index, this,true)"
                    ng-change="ctrl.addQLItoJSON(p)">
                </select>
            </div>
            
            <!-- 999 days -->
            <div class="select-custom-element" ng-if="p.ClaimsGraceDays == '' && (p.ClaimsGraceDayState == 'New York')">
                <select  
                    name="ClaimsGraceDays" id="ClaimsGraceDays" class="slds-select ng-pristine ng-empty ng-valid ng-valid-required ng-touched" 
                    style="font-size:12px;max-width:100%" 
                    ng-model="p.ClaimsGraceDays"
                    ng-options="option.value as option.name for option in ClaimsGraceDays"
                    ng-init="p.ClaimsGraceDays = ClaimsGraceDays[4].value; onSelectItem(control, p, $index, this,true)"
                    ng-change="ctrl.addQLItoJSON(p)">
                </select>
            </div>

            <!-- already populated -->
            <div class="select-custom-element" ng-if="p.ClaimsGraceDays != ''">               
            <select   
            	 name="ClaimsGraceDays" id="ClaimsGraceDays" class="slds-select ng-pristine ng-empty ng-valid ng-valid-required ng-touched" 
                 style="font-size:12px;max-width:100%;" 
                 ng-model="p.ClaimsGraceDays" 
                 ng-options="option.value as option.name for option in ClaimsGraceDays"
                 ng-init="onSelectItem(control, p, $index, this, true)"
                 ng-change="ctrl.addQLItoJSON(p)">
            </select>     
            </div>
                 
          </td>  
          <!--EOF PRDCRM2-1440-->
     </tr>       
     </tbody>
     </table>
 </div>
 
 

 
 <p>&nbsp;</p>
</script>
   
