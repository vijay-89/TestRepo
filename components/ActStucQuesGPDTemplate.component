<script type="text/ng-template" id="vlcActStrcQuesBillingArrangement.html">
<div class="panel panel-default" ng-controller="EmpClassQliGpdController as ctrl"  style="width: 40%">

  <table class="table slds-table slds-table--bordered slds-table--cell-buffer" style="table-layout: auto;" width="100%">
  <thead>
    <tr>
     <!-- Header for checkbox -->
     <th  style="font-size:12px; text-align-last: center;">Select</th>    
     <th class="slds-cell-shrink" style="font-size:12px;">Invoice<br/>Preference</th>      
    </tr>
   </thead>
  
      
  <tbody class='ins' ng-if="control.vlcSI[control.itemsKey].length > 0" >
    <tr ng-repeat="billingEach in ctrl.getInvoivePreferences(control.vlcSI[control.itemsKey])"  >
    
     <td class="slds-cell-shrink">
            <label class="slds-checkbox" style="text-align: center;">
               <input type="checkbox" ng-model='billingEach.vlcSelected' ng-change='onSelectItem(control, billingEach , $index, this);ctrl.checkBillingArrangementSelected();'/>
             <span class="slds-checkbox--faux"></span>

            </label>
        </td>
    
     
          <td style="font-size:12px;">{{billingEach.billing}}</td> 
          
     </tr>
       
     </tbody>
     </table>
</div>


 <p>&nbsp;</p>
</script>

