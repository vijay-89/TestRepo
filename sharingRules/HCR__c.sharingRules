<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>HCR_Sharing_Confidential</fullName>
        <accessLevel>Read</accessLevel>
        <label>HCR Sharing Confidential</label>
        <sharedTo>
            <group>House_Account</group>
        </sharedTo>
        <criteriaItems>
            <field>AccountRecordTypeHCR__c</field>
            <operation>equals</operation>
            <value>Confidential</value>
        </criteriaItems>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>Non_Confidential_Account_Sharing_HCR</fullName>
        <accessLevel>Edit</accessLevel>
        <label>Non-Confidential Account_Sharing_HCR</label>
        <sharedTo>
            <allInternalUsers></allInternalUsers>
        </sharedTo>
        <criteriaItems>
            <field>AccountRecordTypeHCR__c</field>
            <operation>notEqual</operation>
            <value>Confidential</value>
        </criteriaItems>
    </sharingCriteriaRules>
</SharingRules>