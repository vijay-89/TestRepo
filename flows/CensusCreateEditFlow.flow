<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignments>
        <description>Inactivate the Census Records within the loop</description>
        <name>Inactivate_Census_Record</name>
        <label>Inactivate Census Record</label>
        <locationX>685</locationX>
        <locationY>44</locationY>
        <assignmentItems>
            <assignToReference>censusItem.Active_In_Active_Flag__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_Census_List</targetReference>
        </connector>
    </assignments>
    <assignments>
        <description>List of Census Records for update after the inactive assignment</description>
        <name>Update_Census_List</name>
        <label>Update Census List</label>
        <locationX>686</locationX>
        <locationY>193</locationY>
        <assignmentItems>
            <assignToReference>updateCensusList</assignToReference>
            <operator>Add</operator>
            <value>
                <elementReference>censusItem</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Iterate_Census_Records</targetReference>
        </connector>
    </assignments>
    <description>The Flow is invoked by the Process Builder - CensusCreateEditProcess</description>
    <interviewLabel>CensusCreateEditFlow {!$Flow.CurrentDateTime}</interviewLabel>
    <label>CensusCreateEditFlow</label>
    <loops>
        <name>Iterate_Census_Records</name>
        <label>Iterate Census Records</label>
        <locationX>399</locationX>
        <locationY>50</locationY>
        <assignNextValueToReference>censusItem</assignNextValueToReference>
        <collectionReference>censusRecords</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Inactivate_Census_Record</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Update_Census_Records</targetReference>
        </noMoreValuesConnector>
    </loops>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <description>Query All the Census records for a particular case</description>
        <name>Retrieve_Census_Records</name>
        <label>Retrieve Census Records</label>
        <locationX>222</locationX>
        <locationY>50</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Iterate_Census_Records</targetReference>
        </connector>
        <filters>
            <field>Active_In_Active_Flag__c</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>CaseID__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>currentCaseId</elementReference>
            </value>
        </filters>
        <filters>
            <field>Id</field>
            <operator>NotEqualTo</operator>
            <value>
                <elementReference>currentCensusId</elementReference>
            </value>
        </filters>
        <object>Census__c</object>
        <outputReference>censusRecords</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>Active_In_Active_Flag__c</queriedFields>
        <queriedFields>CaseID__c</queriedFields>
    </recordLookups>
    <recordUpdates>
        <name>Update_Census_Records</name>
        <label>Update Census Records</label>
        <locationX>401</locationX>
        <locationY>296</locationY>
        <inputReference>updateCensusList</inputReference>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>50</locationY>
        <connector>
            <targetReference>Retrieve_Census_Records</targetReference>
        </connector>
    </start>
    <status>Active</status>
    <variables>
        <name>censusItem</name>
        <dataType>SObject</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
        <objectType>Census__c</objectType>
    </variables>
    <variables>
        <description>List of extracted census records</description>
        <name>censusRecords</name>
        <dataType>SObject</dataType>
        <isCollection>true</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
        <objectType>Census__c</objectType>
    </variables>
    <variables>
        <description>This variable stores the caseId passed from the process builder</description>
        <name>currentCaseId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <description>Census ID of the original record which triggered the whole process. Passed from the Process Builder</description>
        <name>currentCensusId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>updateCensusList</name>
        <dataType>SObject</dataType>
        <isCollection>true</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
        <objectType>Census__c</objectType>
    </variables>
</Flow>
