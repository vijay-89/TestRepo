    <description>Updates Field With Pending Correction on SME objects, If Stage value changes to Pending Response From Implementations</description>
    <formulas>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>true</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>ISPICKVAL([Pending_Correction__c].Stage__c,&apos;Ready for SMEs&apos;) &amp;&amp; NOT(ISNULL([Pending_Correction__c].Issue_Control__c ))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_11</name>
        <dataType>Boolean</dataType>
        <expression>ISPICKVAL({!myVariable_current.Stage__c},&apos;Ready for SMEs&apos;) &amp;&amp; NOT(ISNULL({!myVariable_current.Issue_Control__c} ))</expression>
    </formulas>
