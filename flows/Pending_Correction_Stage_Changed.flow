        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>OR(ISNEW(), ISCHANGED([Pending_Correction__c].Stage__c ))  &amp;&amp;  ISPICKVAL([Pending_Correction__c].Stage__c,&apos;Pending Response From Implementations&apos;) &amp;&amp; NOT(ISNULL([Pending_Correction__c].Enrollment_and_Billing__c))</stringValue>
            </value>
