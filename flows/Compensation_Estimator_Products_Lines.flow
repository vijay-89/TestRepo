If ( AND(TEXT({!varLoopCommissionsEstimatorVariables.Line_of_Business__c}) = &quot;Life&quot;, {!varIncludeLife} = TRUE), TRUE,
If ( AND(TEXT({!varLoopCommissionsEstimatorVariables.Line_of_Business__c}) = &quot;Disability&quot;, {!varIncludeDisability} = TRUE), TRUE, 
If ( AND(TEXT({!varLoopCommissionsEstimatorVariables.Line_of_Business__c}) = &quot;Supplemental&quot;, {!varIncludeSupplemental} = TRUE), TRUE,  FALSE ))))))</expression>
    </formulas>
