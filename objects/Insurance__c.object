        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Days_Until_Policy_Expiration__c</fullName>
        <description>Number of days until the Next Policy Expiration Date. If today is not before the next evaluation date, then this returns 0.</description>
        <externalId>false</externalId>
        <formula>IF( ISNULL(Collateral_Insurance_Policy_Expiration_D__c) || (Collateral_Insurance_Policy_Expiration_D__c - Today() &lt; 0) , Null, (Collateral_Insurance_Policy_Expiration_D__c - Today()))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Number of days until the policy expires.</inlineHelpText>
        <label>Days Until Policy Expiration</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
