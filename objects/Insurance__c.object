    <fields>
        <fullName>Days_Past_Policy_Expiration__c</fullName>
        <description>Number of days past the policy expiration date. If Today is not past the evaluation date, then this returns &quot;0&quot;.</description>
        <externalId>false</externalId>
        <formula>IF( ISNULL(Collateral_Insurance_Policy_Expiration_D__c) || (Today() - Collateral_Insurance_Policy_Expiration_D__c &lt;= 0), Null , (Today() - Collateral_Insurance_Policy_Expiration_D__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Number of days the policy has been expired.</inlineHelpText>
        <label>Days Past Policy Expiration</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
