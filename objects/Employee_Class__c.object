    <fields>
        <fullName>Formatted_Last_Modified_Date__c</fullName>
        <description>Its a formula field to format last modified date in MM/DD/YYYY format and to display in Account structure Omni script</description>
        <externalId>false</externalId>
        <formula>LastModifiedBy.FirstName + &apos; &apos;+  LastModifiedBy.LastName +&apos; &apos;  +TEXT(MONTH(DATEVALUE(LastModifiedDate)))+&quot;/&quot; +
TEXT(DAY(DATEVALUE(LastModifiedDate)))+&quot;/&quot; +
TEXT(YEAR(DATEVALUE(LastModifiedDate)))+ &quot; &quot; +
IF( OR( VALUE( MID( TEXT( LastModifiedDate ), 12, 2 ) ) = 0, VALUE( MID( TEXT( LastModifiedDate ), 12, 2 ) ) = 12 ), &quot;12&quot;, TEXT( VALUE( MID( TEXT( LastModifiedDate ), 12, 2 ) ) - IF( VALUE( MID( TEXT( LastModifiedDate ), 12, 2 ) ) &lt; 12, 0, 12 ) ) ) &amp; &quot;:&quot; &amp; MID( TEXT( LastModifiedDate ), 15, 2 ) &amp; &quot;:&quot; &amp; MID( TEXT( LastModifiedDate ), 18, 2 ) &amp; &quot; &quot; &amp; IF( VALUE( MID( TEXT( LastModifiedDate ), 12, 2 ) ) &lt; 12, &quot;AM&quot;, &quot;PM&quot; )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Formatted Last Modified Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
