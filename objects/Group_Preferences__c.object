    <fields>
        <fullName>Washout_Code__c</fullName>
        <description>This field is being added to support the Group Load functionality through Customer Master.</description>
        <externalId>false</externalId>
        <inlineHelpText>Washout Code determines if first month premium is billed or not, and if last month premium is credited or not when coverage is terminated.</inlineHelpText>
        <label>Washout Code</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>(01) 1st through 31st - 1st of the following month</fullName>
                    <default>false</default>
                    <label>1st through 31st - 1st of the following month</label>
                </value>
                <value>
                    <fullName>(02) 1st only of the same month; 2nd through 31st - 1st of following month</fullName>
                    <default>false</default>
                    <label>1st only of the same month; 2nd through 31st - 1st of following month</label>
                </value>
                <value>
                    <fullName>(16) 1st through 15th - 1st of same month; 16th through 31st of following month (or ASO group)</fullName>
                    <default>false</default>
                    <label>1st through 15th - 1st of same month; 16th through 31st of following month (or ASO group)</label>
                </value>
                <value>
                    <fullName>(31) 1st through 31st - 1st of same month</fullName>
                    <default>false</default>
                    <label>1st through 31st - 1st of same month</label>
                </value>
                <value>
                    <fullName>(31) COVA/TLC/LODA/VA tech</fullName>
                    <default>false</default>
                    <label>COVA/TLC/LODA/VA tech</label>
                </value>
                <value>
                    <fullName>(16) Georgia - HMO/POS medical products and no COBRA solutions</fullName>
                    <default>false</default>
                    <label>Georgia - HMO/POS medical products and no COBRA solutions</label>
                </value>
                <value>
                    <fullName>(blank) Georgia - Multiple medical products, or includes Specialty Products and COBRA solutions</fullName>
                    <default>false</default>
                    <label>Georgia - Multiple medical products, or includes Specialty Products and COBRA solutions**</label>
                </value>
                <value>
                    <fullName>(blank) Standard - Premium prorated based on 30-day billing cycle**</fullName>
                    <default>false</default>
                    <label>Standard - Premium prorated based on 30-day billing cycle</label>
                </value>
            </valueSetDefinition>
        </valueSet>
