<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This custom setting confirms whether the child task should be automatically created or not for a task in the trigger PMO_Task</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Owner_Queue__c</fullName>
        <description>It stores the ID of the user to which the new task would be assigned</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <label>Owner/Queue</label>
        <length>20</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Preceding_Task__c</fullName>
        <description>It stores the preceding task of the current task.</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <label>Preceding Task</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Task__c</fullName>
        <description>It stores the task name.</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <label>Task</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>PMO_TaskAssignment</label>
    <visibility>Public</visibility>
</CustomObject>