    <validationRules>
        <fullName>Final_Resolution_Required</fullName>
        <active>true</active>
        <description>Description of Final Resolution field is required when Issue Status is &quot;Resolved/Closed&quot;</description>
        <errorConditionFormula>AND(
ISBLANK( Description_of_Final_Resolution__c ),
 ISPICKVAL( Issue_Status__c , &quot;Resolved/Closed&quot;))</errorConditionFormula>
        <errorDisplayField>Description_of_Final_Resolution__c</errorDisplayField>
        <errorMessage>Final Resolution field is required when Issue Status is &quot;Resolved/Closed&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Urgent_Resolution_Required_Date</fullName>
        <active>true</active>
        <description>If Urgent Resolution field is required then Escalated Date field must be populated</description>
        <errorConditionFormula>AND(
 ISBLANK( Escalated_Date__c ) ,
ISPICKVAL( Urgent_Resolution_Required__c , &apos;Yes&apos;))</errorConditionFormula>
        <errorDisplayField>Escalated_Date__c</errorDisplayField>
        <errorMessage>If Urgent Resolution field is required then Escalated Date field must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>What_is_the_number_and_Title_Required</fullName>
        <active>true</active>
        <description>When Connected to Transformation Initiative? is yes then What is the # and Title fields are required.</description>
        <errorConditionFormula>AND(
ISBLANK(  Title__c  ),
ISBLANK(   What_is_the__c   ),
 ISPICKVAL(  Connected_to_Transformation_Initiative__c , &quot;Yes&quot;))</errorConditionFormula>
        <errorMessage>When Connected to Transformation Initiative? is YES then &quot;What is the #&quot; and &quot;Title&quot; fields are required.</errorMessage>
    </validationRules>
