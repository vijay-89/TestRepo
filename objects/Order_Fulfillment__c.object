    <webLinks>
        <fullName>Update_UPS_Status</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Update UPS Status</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/apex.js&quot;)}

var carrier = &quot;{!Order_Fulfillment__c.Carrier__c}&quot;;
if(carrier.includes(&quot;UPS&quot;)) {
var orderIds = new Array(&quot;{!Order_Fulfillment__c.Id}&quot;);
var result = sforce.apex.execute(&quot;UPS&quot;,&quot;updateStatus&quot;,{orderIds:orderIds});
window.location.reload();
}
else {
alert(&quot;Carrier for this order is not UPS&quot;);
}</url>
    </webLinks>
