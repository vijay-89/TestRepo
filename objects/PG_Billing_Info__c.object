    <validationRules>
        <fullName>Lock_Down_PG_Billing_Info</fullName>
        <active>true</active>
        <description>When status moves to &quot;Billing in Process&quot;, lock down entire record with the exception of &quot;Billing Upload Date&quot;</description>
        <errorConditionFormula>AND(
ISPICKVAL(Status__c, &apos;Billing in Process&apos;),
OR(
ISCHANGED( Account_Report__c ),
ISCHANGED( Additional_Required_Approvals__c ),
ISCHANGED( Approval_Notes__c ),
ISCHANGED( Bill_Level_2__c ),
ISCHANGED( Bill_Line_Note__c ),
