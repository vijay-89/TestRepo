    <fields>
        <fullName>ASO_Newborn_Opt_Out_Indicator__c</fullName>
        <description>This field is being added specifically for WGS to support the Group Load functionality through Customer Master.</description>
        <externalId>false</externalId>
        <inlineHelpText>To allow ASO groups to Opt-Out of the Fully Insured mandate to cover newborns for the first 31 days following birth, regardless of whether or not the newborn is physically enrolled.</inlineHelpText>
        <label>ASO Newborn Opt-Out Indicator</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Y</fullName>
                    <default>false</default>
                    <label>Yes, the group opts out</label>
                </value>
                <value>
                    <fullName>N</fullName>
                    <default>false</default>
                    <label>No, the group does not opt out</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
