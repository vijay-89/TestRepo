    <webLinks>
        <fullName>Generate_Branding</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Generate Branding</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)} 
sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot; 
var res = sforce.connection.describeSObjects([&apos;Customer_Portal_Branding__c&apos;]); 
var newId = res[0].keyPrefix; 
var a = new sforce.SObject(&quot;Customer_Portal_Branding__c&quot;); 
a.Id = &quot;{!Customer_Portal_Branding__c.Id}&quot;; 
var executeMethod; 

try { 
executeMethod = sforce.apex.execute(&quot;CustomerPortalBrandingController&quot;, 
&quot;UpdateSR&quot;,{CpBrandingId:a.Id}); 
alert(&apos;Branding has been saved successfully&apos;); 
} catch(err){ 
alert(&quot;An error has occurred uploading your branding template &quot; + 
err); 
} 
sforce.connection.query( 
&quot;SELECT Id, nFORCE__Brand_JSON__c FROM nFORCE__Brand__c WHERE Name = &apos;nSight Financial&apos;&quot;, 
{ onSuccess : handleUpdate , onFailure : doNothing }); 


function handleUpdate(result) { 
result = result.getArray(&apos;records&apos;); 
result[0].nFORCE__Brand_JSON__c = executeMethod; 
sforce.connection.update(result); 

} 
function doNothing(result) { 
}</url>
