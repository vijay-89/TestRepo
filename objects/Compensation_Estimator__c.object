    <fields>
        <fullName>New_Business_Dental_Kicker__c</fullName>
        <externalId>false</externalId>
        <formula>If(  New_Business_Dental_GTA__c = 0, 0, 

 New_Business_Dental_Base_Payment__c / New_Business_Dental_GTA__c *

If(New_Business_Dental_GTA__c &lt;= 0.8, 0,

If(New_Business_Dental_GTA__c &lt;= 1.15, (New_Business_Dental_GTA__c - 0.8)*0.5,

IF(New_Business_Dental_GTA__c &lt;= 1.5, 0.35*0.5 + (New_Business_Dental_GTA__c - 1.15)*1,

(0.35*0.5)+(0.35*1)+((New_Business_Dental_GTA__c -1.5)*1.5))
)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>New Business Dental Kicker</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
