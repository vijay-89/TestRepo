    <validationRules>
        <fullName>Actual_SBC_Completion_Date_Validation</fullName>
        <active>true</active>
        <description>Actual SBC Completion Date cannot be less than Date SBC Requested</description>
        <errorConditionFormula>Actual_SBC_Completion_Date__c &lt; Date_SBC_Requested__c</errorConditionFormula>
        <errorDisplayField>Actual_SBC_Completion_Date__c</errorDisplayField>
        <errorMessage>Actual SBC Completion Date cannot be before Date SBC Requested</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Record_Once_SBC_Completed</fullName>
        <active>true</active>
        <errorConditionFormula>OR
(
AND
(
NOT ISBLANK(PRIORVALUE( Actual_SBC_Completion_Date__c )),
NOT ISBLANK(Actual_SBC_Completion_Date__c ), ($Profile.Name &lt;&gt; &apos;System Administrator&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;API User&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;System Admin&apos; )
),
AND
(
ISCHANGED (Actual_SBC_Completion_Date__c),
ISBLANK (Actual_SBC_Completion_Date__c ),
($Profile.Name &lt;&gt; &apos;System Administrator&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;API User&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;System Admin&apos; ))
)</errorConditionFormula>
        <errorDisplayField>Actual_SBC_Completion_Date__c</errorDisplayField>
        <errorMessage>This request has been completed and cannot be updated</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
