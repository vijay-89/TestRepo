    <validationRules>
        <fullName>Base_Rate_Must_be_a_Number_or_Variable</fullName>
        <active>false</active>
        <description>The Base Rate Must be a Number or set to the term &quot;Variable&quot;</description>
        <errorConditionFormula>AND(NOT(ISBLANK(Base_Rate__c)),
NOT(
OR(Base_Rate__c =&quot;Variable&quot;,
AND(VALUE(Base_Rate__c )&gt;0,VALUE(Base_Rate__c )&lt;20)
)))</errorConditionFormula>
        <errorDisplayField>Base_Rate__c</errorDisplayField>
        <errorMessage>You must enter a value between $0 and $20 for the base rate, OR enter the term &quot;Variable&quot; (without quotations)</errorMessage>
    </validationRules>
